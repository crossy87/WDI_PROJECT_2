<h1>All The Pugs</h1>

<a href="/pugs/new">Add Pug</a><br>

<% pugs.forEach((pug) => { %>
  <img src="<%= pug.pugImages %>">
  <a href="/pugs/<%= pug.id %>"><h2><%= pug.pugName %></h2></a>
  <p>Age: <%= pug.pugAge %></p>
  <p>Costume: <%= pug.costumes %></p>
  <p>Description: <%= pug.pugDescription %></p><br>
  <p>Created By: <%= pug.createdBy %><br>

<% }) %>
